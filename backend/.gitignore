# Node modules
node_modules/

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Build outputs
dist/
build/
coverage/

# Dependency directories
jspm_packages/

# Editor & system files
.DS_Store
Thumbs.db
.vscode/
.idea/

# Optional npm cache directory
.npm/

# Optional eslint cache
.eslintcache

# Optional stylelint cache
.stylelintcache

# Temporary files
temp/
tmp/

# Sequelize migrations (nếu có)
migrations/*-tmp.js
